<Window x:Class="HelloWorld.NavigatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWorld"
        mc:Ignorable="d"
        Title="NavigatorWindow" Height="250" Width="250">
    <Window.Background>
        <ImageBrush ImageSource="wpf-world.png" />
    </Window.Background>
    <StackPanel>
        <TextBlock x:Name="uxPrompt">Enter an address <Run Foreground="Red">(Don't forget http://)</Run></TextBlock>
        <TextBox x:Name="uxURL">

        </TextBox>
        <Button x:Name="uxGo" Click="uxGo_Click">Go</Button>
        
    </StackPanel>
</Window>
